<main>
  <h1>Account Management</h1>

  <!-- Flash messages -->
  <%- messages() %>

  <!-- Errors if any -->
  <% if (typeof errors !== 'undefined' && errors.length > 0) { %>
    <ul class="error-list">
      <% errors.forEach(function(error) { %>
        <li><%= error.msg %></li>
      <% }); %>
    </ul>
  <% } %>

  <p>You're logged in</p>
</main>